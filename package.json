{
  "name": "yoke",
  "version": "0.0.0",
  "main": "dist/index.js",
  "scripts": {
    "ng": "ng",
    "app:build": "tsc --project projects/electron/tsconfig.json",
    "app:start": "concurrently \"npm run app:build\" \"npm run web:start\" \"wait-on http://localhost:4200  && electron-forge start -- --no-sandbox\"",
    "app:package": "npm run app:build && web:build && electron-forge package",
    "app:make": "npm run app:build && web:build && electron-forge make",
    "app:publish": "npm run app:build && web:build && electron-forge publish",
    "web:start": "ng serve",
    "web:build": "ng build",
    "web:watch": "ng build --watch --configuration development",
    "web:test": "ng test",
    "serve:ssr:yoke": "node dist/yoke/server/server.mjs",
    "prepare": "husky"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.6",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.18.2",
    "ngx-electronyzer": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "update-electron-app": "^3.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/publisher-github": "^7.6.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "electron": "^33.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "wait-on": "^8.0.1"
  }
}
